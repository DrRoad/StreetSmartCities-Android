<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:eegeo="http://schemas.android.com/apk/res-auto"
    tools:context="org.vontech.monet.MapActivity">

    <com.eegeo.mapapi.MapView
        android:id="@+id/basic_mapview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        eegeo:camera_target_latitude="33.777643"
        eegeo:camera_target_longitude="-84.395761"
        eegeo:camera_zoom="16"
        />

</android.support.constraint.ConstraintLayout>
